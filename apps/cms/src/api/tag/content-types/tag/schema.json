{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "태그",
    "description": "콘텐츠 태깅을 위한 태그"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "terms": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::term.term",
      "mappedBy": "tags"
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "tags"
    },
    "downloads": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::download.download",
      "mappedBy": "tags"
    }
  }
}
